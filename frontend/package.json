{
    "name": "audio-braille-patterns",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "ngVersion": "ng --version",
        "start": "ng serve -o",
        "upgrade:ng": "npm run ng -- update @angular/cli@^9 @angular/core@^9",
        "build": "ng build --prod",
        "add-google-analytics-to-index-html": "node ./buildScript/addGoogleAnalytics.js && cat ./src/index.html",
        "test": "ng test",
        "test-ci": "cross-env CI=true ng test --watch=false",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "ngcc",
        "netlify": "netlify",
        "netlify:deploy:draft": "npm run netlify -- deploy --auth ${NETLIFY_AUTH_TOKEN} --dir ./dist --site ${NETLIFY_SITE_ID}",
        "netlify:deploy:prod": "npm run netlify -- deploy --prod --auth ${NETLIFY_AUTH_TOKEN} --dir ./dist --site ${NETLIFY_SITE_ID}",
        "netlify:deploy:local:draft": "export GITHUB_SHA=123DraftTest321 && npm run env-cmd -- -f .env --use-shell 'npm run netlify:deploy:draft -- --message \"Deployed locally from GithubActions ${GITHUB_SHA}\"'",
        "netlify:deploy:local:prod": "export GITHUB_SHA=123ProdTest321 npm run env-cmd -- -f .env --use-shell 'npm run netlify:deploy:prod -- --message \"Deployed locally from GithubActions ${GITHUB_SHA}\"'",
        "env-cmd": "env-cmd"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "9.1.4",
        "@angular/cdk": "9.2.2",
        "@angular/common": "9.1.4",
        "@angular/compiler": "9.1.4",
        "@angular/core": "9.1.4",
        "@angular/flex-layout": "9.0.0-beta.29",
        "@angular/forms": "9.1.4",
        "@angular/material": "^9.2.2",
        "@angular/platform-browser": "9.1.4",
        "@angular/platform-browser-dynamic": "9.1.4",
        "@angular/router": "9.1.4",
        "@ngx-translate/core": "12.1.2",
        "@ngx-translate/http-loader": "4.0.0",
        "chart.js": "2.9.3",
        "core-js": "3.6.5",
        "ng2-charts": "2.3.0",
        "rxjs": "6.5.5",
        "web-animations-js": "^2.3.2",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.4",
        "@angular/cli": "9.1.4",
        "@angular/compiler-cli": "9.1.4",
        "@angular/language-service": "9.1.4",
        "@types/jasmine": "3.5.10",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "^13.11.1",
        "codelyzer": "^5.2.2",
        "cross-env": "^7.0.2",
        "env-cmd": "^10.1.0",
        "jasmine-core": "3.5.0",
        "jasmine-spec-reporter": "5.0.1",
        "karma": "^5.0.0",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.1",
        "karma-jasmine": "3.1.1",
        "karma-jasmine-html-reporter": "1.5.3",
        "netlify-cli": "^2.48.0",
        "node-pre-gyp": "^0.14.0",
        "protractor": "5.4.3",
        "shelljs": "^0.8.4",
        "ts-node": "8.8.2",
        "tslib": "1.11.1",
        "tslint": "6.1.1",
        "typescript": "3.8.3"
    }
}
