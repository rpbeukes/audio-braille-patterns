{
  "name": "audio-braille-patterns",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "ngVersion": "ng --version",
    "start": "ng serve -o",
    "upgrade:ng": "npm run ng -- update @angular/cli@^9 @angular/core@^9",
    "build": "ng build --prod",
    "test": "ng test",
    "test-ci": "cross-env CI=true ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "netlify": "netlify",
    "netlify:deploy:draft": "npm run netlify -- deploy --auth ${NETLIFY_AUTH_TOKEN} --dir ./dist --site ${NETLIFY_SITE_ID}",
    "netlify:deploy:prod": "npm run netlify -- deploy --prod --auth ${NETLIFY_AUTH_TOKEN} --dir ./dist --site ${NETLIFY_SITE_ID}",
    "netlify:deploy:local:draft": "export GITHUB_SHA=123DraftTest321 && npm run env-cmd -- -f .env --use-shell 'npm run netlify:deploy:draft -- --message \"Deployed locally from GithubActions ${GITHUB_SHA}\"'",
    "netlify:deploy:local:prod": "export GITHUB_SHA=123ProdTest321 npm run env-cmd -- -f .env --use-shell 'npm run netlify:deploy:prod -- --message \"Deployed locally from GithubActions ${GITHUB_SHA}\"'",
    "env-cmd": "env-cmd"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.6",
    "@angular/cdk": "9.2.3",
    "@angular/common": "9.1.6",
    "@angular/compiler": "9.1.6",
    "@angular/core": "10.2.5",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.1.6",
    "@angular/material": "9.2.3",
    "@angular/platform-browser": "9.1.6",
    "@angular/platform-browser-dynamic": "9.1.6",
    "@angular/router": "9.1.6",
    "core-js": "3.6.5",
    "rxjs": "6.5.5",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "9.1.5",
    "@angular/compiler-cli": "9.1.6",
    "@angular/language-service": "9.1.6",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^13.11.1",
    "codelyzer": "^5.2.2",
    "cross-env": "^7.0.2",
    "env-cmd": "^10.1.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.1",
    "karma": "^5.0.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "netlify-cli": "^2.48.0",
    "node-pre-gyp": "^0.14.0",
    "protractor": "5.4.3",
    "shelljs": "^0.8.4",
    "ts-node": "8.8.2",
    "tslib": "1.11.1",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  }
}
